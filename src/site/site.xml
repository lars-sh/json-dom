<?xml version="1.0" encoding="UTF-8"?>
<project name="${project.name}" xmlns="http://maven.apache.org/DECORATION/1.8.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/DECORATION/1.8.0 https://maven.apache.org/xsd/decoration-1.8.0.xsd">
	<skin>
		<groupId>org.apache.maven.skins</groupId>
		<artifactId>maven-fluido-skin</artifactId>
		<version>${maven-fluido-skin.version}</version>
	</skin>
	<custom>
		<fluidoSkin>
			<sideBarEnabled>true</sideBarEnabled>
			<gitHub>
				<projectId>${parent-pom.github.organization}/${parent-pom.github.project}</projectId>
				<ribbonOrientation>right</ribbonOrientation>
			</gitHub>
		</fluidoSkin>
	</custom>
	<body>
		<menu name="Overview">
			<item name="Introduction" href="index.html" />
			<item name="Changelog" href="CHANGELOG.html" />
			<item name="JavaDoc" href="apidocs" />
		</menu>
		<menu ref="modules" />
		<menu ref="reports" />
	</body>
</project>
